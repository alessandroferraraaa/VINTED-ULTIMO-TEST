# DISCORD WEBHOOK (optional)
# Copia l'URL del webhook dal tuo server Discord
# Formato: https://discord.com/api/webhooks/YOUR_ID/YOUR_TOKEN
DISCORD_WEBHOOK_URL=

# TELEGRAM BOT (optional)
# Token da @BotFather su Telegram
TELEGRAM_BOT_TOKEN=

# TELEGRAM CHAT ID
# Ottieni l'ID con @userinfobot o @get_id_bot
TELEGRAM_CHAT_ID=

# VINTED PARAMETERS
# Intervallo di controllo in secondi (default: 60)
CHECK_INTERVAL=60

# Database name (default: vinted_bot.db)
DB_NAME=vinted_bot.db

# Log level (DEBUG, INFO, WARNING, ERROR, CRITICAL)
LOG_LEVEL=INFO
